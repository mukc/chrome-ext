{
  "manifest_version": 2,
  "name": "Chrome Extension - JS",
  "version": "1.0.0",
  "content_scripts": [
    {
      "matches": ["https://www.cnbc.com/us-market-movers/", "https://forms.zohopublic.in/developers/form/*"],
      "js": ["content.js"]
    }
  ],
  "permissions": [
    "contextMenus",
    "tabs"
  ],
  "browser_action": {
    "default_popup": "popup.html",
    "default_title": "Chrome Ext"
  }
}