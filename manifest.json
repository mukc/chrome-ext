{
  "manifest_version": 2,
  "name": "Chrome Extension - JS",
  "description": "A simple project to open multi tabs and fill form data written in JavaScript",
  "version": "1.0.0",
  "content_scripts": [
    {
      "matches": ["https://www.cnbc.com/us-market-movers/", "https://forms.zohopublic.in/developers/form/*"],
      "js": ["content.js"]
    }
  ],
  "permissions": [
    "tabs"
  ],
  "browser_action": {
    "default_popup": "popup.html",
    "default_title": "Chrome Ext"
  },
  "background": {
    "scripts": [
      "background.js"
    ],
    "persistent": false
  }
}
